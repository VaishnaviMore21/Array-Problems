class Solution {
    public int majorityElement(int[] nums) {
        int n=nums.length;
        int cnt=0;
        for(int i=0;i<n;i++)
        {
           for(int j=0;j<n;j++)
           {
            if(nums[j]==nums[i])
            {
                cnt++;
            }
           }
           int maj=n/2;
           if(cnt>n)
           
            return nums[i];
           
        }
      
    

}
}
